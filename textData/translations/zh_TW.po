#: command.op.sd
msgid "Gain matrix admin"
msgstr "獲得Matrix管理員權限"

#: command.deop.sd
msgid "Remove your admin perm"
msgstr "移除您的管理員權限"

#: command.vanish.sd
msgid "(Un)vanish yourself"
msgstr "（隱身/顯現）"

#: command.about.sd
msgid "Get info of Matrix-anti"
msgstr "獲取Matrix反外掛的信息"

#: command.crash.description
msgid "Crash a player."
msgstr "使玩家崩潰。"

#: module.mobaura.name
msgid "Anti Mob Aura"
msgstr "反生物光環"

#: module.mobaura.description
msgid "Spawn dummy when player attack a mob, checks if the player is attacking the dummy."
msgstr "玩家攻擊生物時生成虛擬目標，檢查玩家是否在攻擊虛擬目標。"

#: module.elytraFly.name
msgid "Anti Elytra Fly"
msgstr "反翅膀飛行"

#: module.elytraFly.description
msgid "Prevent the player from using the illegal elytra flying."
msgstr "防止玩家使用非法的翅膀飛行。"

#: module.autotool.name
msgid "Anti Auto Tool"
msgstr "禁上自動工具"

#: module.autotool.description
msgid "Checks if player is using autoTool hack to break blocks."
msgstr "檢查玩家是否使用自動工具作弊來破壞方塊。"

#: module.deviceban.name
msgid "Device Ban"
msgstr "設備封禁"

#: module.deviceban.description
msgid "Prevent the player from using a banned device to join the server."
msgstr "防止玩家使用被封禁的設備加入服務器。"

#: command.ban.finish
msgid "§bMatrix§a+ §7>§g %1 has been banned by %2."
msgstr "bMatrix§a+ §7>§g %1 已被 %2 封禁。"

#: command.unban.finish
msgid "§bMatrix§a+ §7>§g %1 has been unbanned by %2."
msgstr "bMatrix§a+ §7>§g %1 已被 %2 解封。"

#: command.kick.finish
msgid "§bMatrix§a+ §7>§g %1 has been kicked by %2."
msgstr "bMatrix§a+ §7>§g %1 已被 %2 踢出。"

#: command.mute.finish
msgid "§bMatrix§a+ §7>§g %1 has been muted by %2."
msgstr "bMatrix§a+ §7>§g %1 已被 %2 禁言。"

#: command.unmute.finish
msgid "§bMatrix§a+ §7>§g %1 has been unmuted by %2."
msgstr "bMatrix§a+ §7>§g %1 已被 %2 解除禁言。"

#: command.warn.finish
msgid "§bMatrix§a+ §7>§g %1 has been warned by %2. §7(Warns: %3)"
msgstr "bMatrix§a+ §7>§g %1 已被 %2 警告。 §7（警告次數: %3）"

#: command.clearwarn.finish
msgid "§bMatrix§a+ §7> §gAll the warnings of %1 has been cleared by %2."
msgstr "bMatrix§a+ §7> §g %1 的所有警告已被 %2 清除。"

#: command.unban.notfound
msgid "§bMatrix§a+ §7> §cThe player %1 has not been banned yet. You can use -banlist to see the list of banned players."
msgstr "bMatrix§a+ §7> §c玩家 %1 還沒有被封禁。您可以使用 -banlist 查看被封禁玩家的列表。"

#: command.unmute.notfound
msgid "§bMatrix§a+ §7> §cThe player %1 has not been muted yet."
msgstr "bMatrix§a+ §7> §c玩家 %1 還沒有被禁言。"

#: command.moderation.ban.expired
msgid "§bMatrix§a+ §7> §aYour ban has expired!"
msgstr "bMatrix§a+ §7> §a您的封禁已過期！"

#: command.moderation.mute.expired
msgid "§bMatrix§a+ §7> §aYou're no longer to be muted"
msgstr "bMatrix§a+ §7> §a您不再被禁言"

